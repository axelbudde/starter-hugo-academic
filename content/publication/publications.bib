	journal = {Allergo J Int},
	author = {Klimek, Ludger and Novak, Natalija and Hamelmann, Eckard and Werfel, Thomas and Wagenmann, Martin and Taube, Christian and Bauer, Andrea and Merk, Hans and Rabe, Uta and Jung, Kirsten and Schlenter, Wolfgang and Ring, Johannes and Chaker, Adam and Wehrmann, Wolfgang and Becker, Sven and Mülleneisen, Norbert and Nemat, Katja and Czech, Wolfgang and Wrede, Holger and Brehler, Randolf and Fuchs, Thomas and Jakob, Thilo and Ankermann, Tobias and Schmidt, Sebastian M. and Gerstlauer, Michael and Vogelberg, Christian and Zuberbier, Thomas and Hartmann, Karin and Worm, Margitta},
	month = feb,
