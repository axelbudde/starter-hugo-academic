	note = {Publisher: Radiological Society of North America},
	pages = {E419--E425},
	file = {Kravchenko et al. - 2021 - Cardiac MRI in Patients with Prolonged Cardiorespi.pdf:files/2878/Kravchenko et al. - 2021 - Cardiac MRI in Patients with Prolonged Cardiorespi.pdf:application/pdf},
