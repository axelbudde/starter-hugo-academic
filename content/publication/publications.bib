	pages = {2007},
	file = {Liskova et al. - 2021 - Mitochondriopathies as a Clue to Systemic Disorder.pdf:files/584/Liskova et al. - 2021 - Mitochondriopathies as a Clue to Systemic Disorder.pdf:application/pdf;Snapshot:files/585/2007.html:text/html},
