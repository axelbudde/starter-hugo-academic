@article{rieke_natural_2022,
	title = {Natural {Killer} {Cell}-{Mediated} {Antibody}-{Dependent} {Cellular} {Cytotoxicity} {Against} {SARS}-{CoV}-2 {After} {Natural} {Infection} {Is} {More} {Potent} {Than} {After} {Vaccination}},
