	pages = {6545},
	file = {Schmuck et al. - 2022 - Individual Stress Burden and Mental Health in Heal.pdf:files/3279/Schmuck et al. - 2022 - Individual Stress Burden and Mental Health in Heal.pdf:application/pdf},
