	journal = {JMIR Research Protocols},
	author = {Widmann, Catherine N. and Wieberneit, Michelle and Bieler, Luzie and Bernsen, Sarah and Gräfenkämper, Robin and Brosseron, Frederic and Schmeel, Carsten and Tacik, Pawel and Skowasch, Dirk and Radbruch, Alexander and Heneka, Michael T.},
	month = nov,
