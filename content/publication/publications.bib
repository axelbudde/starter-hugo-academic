	author = {Ryom, Lene and De Miguel, Rosa and Cotter, Aoife Grace and Podlekareva, Daria and Beguelin, Charles and Waalewijn, Hylke and Arribas, Josè R and Mallon, Patrick W. G. and Marzolini, Catia and Kirk, Ole and Bamford, Alasdair and Rauch, Andri and Molina, Jean Michel and Kowalska, Justyna Dominika and Guaraldi, Giovanni and Winston, Alan and Boesecke, Christoph and Cinque, Paola and Welch, Steven and Collins, Simon and Behrens, Georg M. N. and Board, the EACS Governing},
	year = {2022},
