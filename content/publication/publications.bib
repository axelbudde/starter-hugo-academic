	pmid = {35474750},
	pmcid = {PMC8872836},
	pages = {100560},
	file = {Giamarellos-Bourboulis et al. - 2022 - Development and validation of SCOPE score A clini.pdf:files/2705/Giamarellos-Bourboulis et al. - 2022 - Development and validation of SCOPE score A clini.pdf:application/pdf},
}

