	keywords = {SARS-CoV, aptamer, SELEX, viral infection},
	file = {Mayer et al. - 2021 - A SARS-CoV-2 spike binding DNA aptamer that inhibi.pdf:files/598/Mayer et al. - 2021 - A SARS-CoV-2 spike binding DNA aptamer that inhibi.pdf:application/pdf;Snapshot:files/597/anie.html:text/html},
