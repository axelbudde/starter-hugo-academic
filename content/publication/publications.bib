	urldate = {2022-05-30},
	journal = {International Journal of Infectious Diseases},
	author = {Weinstock, Leonard B. and Brook, Jill B. and Walters, Arthur S. and Goris, Ashleigh and Afrin, Lawrence B. and Molderings, Gerhard J.},
	month = nov,
	year = {2021},
	keywords = {COVID-19, fatigue, Long-COVID, mast cell activation},
	pages = {217--226},
	file = {ScienceDirect Snapshot:files/2889/S1201971221007517.html:text/html;Weinstock et al. - 2021 - Mast cell activation symptoms are prevalent in Lon.pdf:files/2888/Weinstock et al. - 2021 - Mast cell activation symptoms are prevalent in Lon.pdf:application/pdf},
}

