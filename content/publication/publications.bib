	author = {Liskova, Alena and Koklesova, Lenka and Samec, Marek and Abdellatif, Basma and Zhai, Kevin and Siddiqui, Manaal and Šudomová, Miroslava and Hassan, Sherif T.S. and Kudela, Erik and Biringer, Kamil and Giordano, Frank A. and Büsselberg, Dietrich and Golubnitschaja, Olga and Kubatka, Peter},
	month = sep,
