	pages = {108160},
	file = {von Wrede et al. - 2021 - COVID-19 vaccination in patients with epilepsy Fi.pdf:files/2847/von Wrede et al. - 2021 - COVID-19 vaccination in patients with epilepsy Fi.pdf:application/pdf},
