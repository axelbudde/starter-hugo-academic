 issn = {1471-2288},
 journal = {BMC Medical Research Methodology},
 keywords = {COVID-19, Observational studies, Reporting quality, STROBE statement},
 month = {January},
 number = {1},
 pages = {23},
 title = {Poor reporting quality of observational clinical studies comparing treatments of COVID-19 – a retrospective cross-sectional study},
 url = {https://doi.org/10.1186/s12874-021-01501-9},
